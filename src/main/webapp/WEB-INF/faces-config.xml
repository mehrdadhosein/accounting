<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
		"-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
		"http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" version="2.0">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		<navigation-handler>ir.serajsamaneh.core.base.SerajNavigationHandler</navigation-handler>
		<message-bundle>javax.faces.FacesMessages</message-bundle>
		<locale-config>
			<default-locale>fa</default-locale>
		</locale-config>

		<action-listener>
			ir.serajsamaneh.core.base.SerajProcessAction
		</action-listener>
		
				
	</application>
      <converter>
           <converter-id>seraj.converter.Enum</converter-id>
            <converter-class>ir.serajsamaneh.core.converter.EnumTypeConverter</converter-class>
      </converter>
       
       
       
       
	<converter>
           <converter-id>seraj.converter.CurrencyToNumber</converter-id>
            <converter-class>ir.serajsamaneh.core.converter.CoreNumberFormatter</converter-class>
      </converter>
	<converter>
		<converter-id>seraj.converter.CSVtoListOfString</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.CSVtoListOfStringConverter</converter-class>
	</converter>        
	<converter>
		<converter-id>seraj.converter.CSVtoListOfStringV2</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.CSVtoListOfStringConverterV2</converter-class>
	</converter>        
      <converter>
		<converter-id>seraj.converter.CSVtoListOfLong</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.CSVtoListOfLongConverter</converter-class>
	 </converter>
      
      <converter>
		<converter-id>seraj.converter.ListOfLongToCSV</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.ListOfLongConverter</converter-class>
	 </converter>
	 <converter>
		<converter-id>seraj.converter.Jensiyat</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.JensiyatConverter</converter-class>
	</converter>
	<converter>
		<converter-id>seraj.converter.StructureType</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.StructureTypeConverter</converter-class>
	</converter>
	<converter>
		<converter-id>seraj.converter.YesNo</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.YesNoConverter</converter-class>
	</converter>
	<converter>
		<converter-id>StringConverter</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.StringConverter</converter-class>
	</converter>

	<converter>
		<converter-id>seraj.converter.EnumToString</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.EnumTypeConverterToString</converter-class>
	</converter>
	 
	<converter>
		<converter-id>seraj.converter.BigDecimal</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.BigDecimalConverter</converter-class>
	</converter>
	
	<converter>
		<converter-id>seraj.converter.ConvertMinutesToTime</converter-id>
		<converter-class>ir.serajsamaneh.core.converter.ConvertMinutesToTimeConverter</converter-class>
	</converter>
	 
	<converter>
		<converter-for-class>java.util.Date</converter-for-class>
		<converter-class>serajcomponent.JSFDateConverter</converter-class>
	</converter>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>accessDenied</from-outcome>
			<to-view-id>/access/accessDenied.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>licenseExpired</from-outcome>
			<to-view-id>/license/licenseExpired.view.xhml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>licenseExpired</from-outcome>
			<to-view-id>/license/licenseExpired.view.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<component>
		<component-type>DateTimePicker</component-type>
		<component-class>serajcomponent.DateTimePicker</component-class>
	</component>

	<managed-bean>
		<managed-bean-name>report</managed-bean-name>
		<managed-bean-class>ir.serajsamaneh.model.reportDesign.ReportGenerator</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		
	</managed-bean>

	<component>
		<component-type>SerajEditor</component-type>
		<component-class>serajcomponent.editor.SerajEditor</component-class>
	</component>
	<lifecycle>
		<phase-listener>ir.serajsamaneh.core.security.PageAccessControlPhaseListener</phase-listener>
	</lifecycle>
	<converter>
		<converter-id>seraj.converter.KalaQuality</converter-id>
		<converter-class>ir.serajsamaneh.anbar.converter.KalaQualityConverter</converter-class>
	</converter>

	<converter>
		<converter-id>seraj.converter.KalaRegulationState</converter-id>
		<converter-class>ir.serajsamaneh.anbar.converter.KalaRegulationStateConverter</converter-class>
	</converter>
	<converter>
		<converter-id>seraj.converter.NoeMalekiyat</converter-id>
		<converter-class>ir.serajsamaneh.anbar.converter.NoeMalekiyatConverter</converter-class>
	</converter>
	<converter>
		<converter-id>seraj.converter.DarkhastResultState</converter-id>
		<converter-class>ir.serajsamaneh.anbar.converter.DarkhastResultStateConverter</converter-class>
	</converter>
	<converter>
		<converter-id>seraj.converter.KalaType</converter-id>
		<converter-class>ir.serajsamaneh.anbar.converter.KalaTypeConverter</converter-class>
	</converter>
	<converter>
		<converter-id>seraj.converter.AnbargardaniType</converter-id>
		<converter-class>ir.serajsamaneh.anbar.converter.AnbargardaniTypeConverter</converter-class>
	</converter>
	<converter>
           <converter-id>seraj.converter.accountingNumberFormatter</converter-id>
            <converter-class>ir.serajsamaneh.accounting.converter.AccountingNumberFormatter</converter-class>
      </converter></faces-config>