<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	
	xsi:schemaLocation="http://www.springframework.org/schema/beans
              http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
              http://www.springframework.org/schema/aop
              http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
              http://www.springframework.org/schema/tx
              http://www.springframework.org/schema/tx/spring-tx-2.0.xsd">
	
	<bean id="moeenAccountingMarkazTemplateDAO" class="ir.serajsamaneh.accounting.moeenaccountingmarkaztemplate.MoeenAccountingMarkazTemplateDAO">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	
	<bean id="moeenAccountingMarkazTemplateService" class="ir.serajsamaneh.accounting.moeenaccountingmarkaztemplate.MoeenAccountingMarkazTemplateService"
		autowire="byName" >
		<property name="moeenAccountingMarkazTemplateDAO" ref="moeenAccountingMarkazTemplateDAO" />
	</bean>
		
	<bean id="moeenAccountingMarkazTemplate" class="ir.serajsamaneh.accounting.moeenaccountingmarkaztemplate.MoeenAccountingMarkazTemplateForm" autowire="byName"
		scope="request" >
		<property name="moeenAccountingMarkazTemplateService" ref="moeenAccountingMarkazTemplateService" />
	</bean>
</beans>